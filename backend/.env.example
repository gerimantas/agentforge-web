# Environment variables for AgentForge-Web Backend
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@db:5432/agentforge

# Security Configuration
SECRET_KEY=your-super-secret-key-change-this-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=30
ALGORITHM=HS256

# CORS Configuration
ALLOWED_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]

# AgentForge Configuration
OPENAI_API_KEY=your-openai-api-key-here
SERPER_API_KEY=your-serper-api-key-here
MAX_AGENT_ITERATIONS=3
AGENT_TIMEOUT=300

# Application Configuration
PROJECT_NAME=AgentForge Web
VERSION=1.0.0
DEBUG=True

# Development Database (alternative)
# DATABASE_URL=sqlite:///./agentforge.db
